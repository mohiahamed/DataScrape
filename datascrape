        "# Import required libraries\n",
        "import requests\n",
        "from bs4 import BeautifulSoup\n",
        "import pandas as pd"
      ],
      "metadata": {
        "id": "F3xi-p6Rr5eD"
      },
      "execution_count": 1,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "# Send GET request to website\n",
        "response = requests.get('http://books.toscrape.com/')\n"
      ],
      "metadata": {
        "id": "5oUOoLgGr846"
      },
      "execution_count": 2,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "# Parse HTML content\n",
        "soup = BeautifulSoup(response.content, 'html.parser')"
      ],
      "metadata": {
        "id": "Ts_1aDbor8lC"
      },
      "execution_count": 3,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "# Extract data\n",
        "books = []\n",
        "for article in soup.select('article.product_pod'):\n",
        "    name = article.h3.a['title']\n",
        "    price = article.select('p.price_color')[0].get_text()\n",
        "    availability = article.select('p.availability')[0].get_text().strip()\n",
        "    books.append({'name': name, 'price': price, 'availability': availability})\n"
      ],
      "metadata": {
        "id": "LgEkenJZsELs"
      },
      "execution_count": 4,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "# Create DataFrame\n",
        "df = pd.DataFrame(books)\n"
      ],
      "metadata": {
        "id": "FqR3PPGhsEUC"
      },
      "execution_count": 5,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "# Export Dataframe\n",
        "df.to_csv(\"books.csv\", index=False)\n",
        "df.to_json(\"books.json\")"
      ],
